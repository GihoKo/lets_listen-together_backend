generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id String @id @default(uuid())
  nickName String 
  profileImage String
  Channel Channel[]
}

model Channel {
  id String @id @default(uuid())
  name String
  description String
  image String
  tags Json
  user User[]
  music Music[]
}

model Music {
  id String @id @default(uuid())
  title String
  artist String
  url String
  channelId String
  channel Channel @relation(fields: [channelId], references: [id])
}
